msgstr "Leite Energie von den Schilden auf die Waffen."


# ========== ./data/scripts/entity/story/artifactdeliverybeacon.lua ==========
#: ./data/scripts/entity/story/artifactdeliverybeacon.lua:
msgid "Scanner Beacon"
msgstr "Scanner-Außenposten"

#: ./data/scripts/entity/story/artifactdeliverybeacon.lua:
msgid "Scanning..."
msgstr "Scanne..."

#: ./data/scripts/entity/story/artifactdeliverybeacon.lua:
msgid "Success. Calling the collector."
msgstr "Erfolg. Rufe den Sammler."

#: ./data/scripts/entity/story/artifactdeliverybeacon.lua:
msgid "Please be patient. Extraction will begin soon."
msgstr "Einen Moment bitte. Die Entnahme beginnt bald."

#: ./data/scripts/entity/story/artifactdeliverybeacon.lua:
